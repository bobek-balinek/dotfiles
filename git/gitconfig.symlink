[core]
  excludesfile = ~/.gitignore
  autocrlf = input

[credential]
  helper = osxkeychain

[push]
  default = current

[user]
  name = Bobby Bobak
  email = bobak.art@gmail.com

[color]
  diff = auto
  status = auto
  branch = auto
  ui = true

[help]
  autocorrect = 1

[pretty]
  colored = format:%C(yellow)%h%Creset <%an> %s %C(green)(%cr) %Creset
  graph = format:%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset

[init]
	defaultBranch = main